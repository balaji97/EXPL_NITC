decl
    int a, i, b, n;
enddecl
begin
    a = 0;
    b = 1;
    read(n);
    i = 0;
    if(n > 0) then
        write(a);
    endif;
    if(n > 1) then
        write(b);
    endif;
    n = n-2;
    while(i < n) do
        write(a+b);
        b = a+b;
        a = b-a;
        i = i+1;
    endwhile;
    
end
